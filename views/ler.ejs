<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Read</title>
</head>
<style>
    body{
        display: flex;
        justify-content: space-around;
    }
    table, th{
        border: 1px solid black;
    }
    .credit{
        background-color: red;
    }
    .debit{
        background-color: green;
    }
</style>
<body onload="ale()">
    <table class="box">
        <h1>Crédito</h1>
        <% credito.forEach(function(conta){ %>
            <tr class="box">
                <th><%= conta.Descricao %></th>
                <th class="credit"><%= conta.Valor %></th>           
            </tr>
        <% }); %>
    </table>

    <table class="box">
        <h1>Débito</h1>
        <% debito.forEach(function(conta){ %>
            <tr class="box">
                <th><%= conta.Descricao %></th>
                <th class="debit"><%= conta.Valor %></th>           
            </tr>
        <% }); %>
    </table>

<script>

    function ale(){
        var valores = document.getElementsByClassName("credit")
            for (i = 0; i < valores.length; i++) {
                console.log( valores[i].innerHTML);
    }
}


</script>
</body>
</html>