<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Read</title>
</head>
<body>
    <table class="box">
        <% credito.forEach(function(conta){ %>
            <tr class="box">
                <th><%= conta.Descricao %></th>
                <th><%= conta.Valor %></th>           
            </tr>
        <% }); %>
    </table>

    <table class="box">
        <% debito.forEach(function(conta){ %>
            <tr class="box">
                <th><%= conta.Descricao %></th>
                <th><%= conta.Valor %></th>           
            </tr>
        <% }); %>
    </table>


</body>
</html>